         <title-group>
            <article-title> La pièce manquante de la sociologie du choix rationnel </article-title>
         </title-group>
         <contrib-group>
            <contrib contrib-type="author">
               <string-name xmlns:xlink="http://www.w3.org/1999/xlink">
                  <given-names>Olivier</given-names>
                  <surname>Favereau</surname>
