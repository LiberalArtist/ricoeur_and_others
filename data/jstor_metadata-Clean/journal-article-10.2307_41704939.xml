         <title-group>
            <article-title>Poétique et phénoménologie: le point de vue et la perception</article-title>
         </title-group>
         <contrib-group>
            <contrib>
               <string-name xmlns:xlink="http://www.w3.org/1999/xlink">
                  <given-names>FLORENCE</given-names>
