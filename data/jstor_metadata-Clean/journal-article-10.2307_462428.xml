         <title-group>
            <article-title>Joseph's Bones and the Resurrection of the Text: Remembering in the Bible</article-title>
         </title-group>
         <contrib-group>
            <contrib contrib-type="author">
               <string-name xmlns:xlink="http://www.w3.org/1999/xlink">
                  <given-names>Regina M.</given-names>
