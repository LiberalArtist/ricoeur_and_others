            <day>1</day>
            <month>2</month>
            <year>1984</year>
         </pub-date>
         <volume>11</volume>
         <issue>1</issue>
         <issue-id>i226343</issue-id>
         <fpage>98</fpage>
         <lpage>117</lpage>
