         <title-group>
            <article-title>Woman as Tribute, Woman as Flower: Images of "Woman" in Weddings in North and South India</article-title>
         </title-group>
         <contrib-group>
            <contrib contrib-type="author">
               <string-name>
                  <given-names>Pauline</given-names>
                  <surname>Kolenda</surname>
               </string-name>
            </contrib>
         </contrib-group>
