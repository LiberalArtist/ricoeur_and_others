         <title-group>
            <article-title>Rethinking the Political: Some Heideggerian Contributions</article-title>
         </title-group>
         <contrib-group>
            <contrib contrib-type="author">
               <string-name xmlns:xlink="http://www.w3.org/1999/xlink">
                  <given-names>Fred</given-names>
                  <surname>Dallmayr</surname>
